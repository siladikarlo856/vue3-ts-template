{
  "name": "vue3-ts-template",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "start": "webpack-dev-server --open --mode development",
    "start:live": "webpack-dev-server --open --mode development --live-reload",
    "format": "prettier . --write",
    "lint": "eslint . --ext  .js,.ts,.vue,.json",
    "lint-and-fix": "eslint . --ext .js,.ts,.vue,.json --fix",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "prepare": "husky install",
    "storybook": "start-storybook -p 9000 --https --ssl-cert ./localhostDev.crt --ssl-key ./localhostDev.key",
    "build-storybook": "build-storybook"
  },
  "license": "MIT",
  "author": {
    "name": "Jack Herrington",
    "email": "jherr@pobox.com"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-events": "^6.2.9",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.14",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vue/compiler-sfc": "^3.2.19",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.7",
    "css-loader": "^6.3.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^8.0.2",
    "mocha": "^10.1.0",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.8.0",
    "style-loader": "^3.3.0",
    "tailwindcss": "^2.0.2",
    "ts-loader": "^8.0.12",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.3",
    "vue-loader": "^16.8.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@mdi/font": "^7.0.96",
    "@pidpm/component-library": "^3.0.147",
    "@vuepic/vue-datepicker": "^3.6.1",
    "@vueuse/core": "^9.6.0",
    "axios": "^1.2.0",
    "chart.js": "^4.0.1",
    "dayjs": "^1.11.7",
    "devextreme": "^22.1.6",
